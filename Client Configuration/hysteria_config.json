//Hysteria 的 v1 版配置示例
{
  "server": "xx.yy:2083", //修改为服务端分配的域名及端口
  "protocol": "udp", //修改为服务端配置的传输协议
  "acl": "route_list.acl", //修改为本地配置的 ACL 文件（选配）。若无此文件，那么必须删除此条参数。
  "obfs": "obfs2083", //修改为服务端配置的混淆密码
  "auth": "[BASE64]", //修改为服务端配置的 Base64 验证密钥（选配）。若服务端无此部分配置，那么必须删除此条参数。
  "auth_str": "diy2083", //修改为服务端配置的字符串验证密钥（选配，和上面的选项二选一。）。若服务端无此部分配置，那么必须删除此条参数。
  "alpn": "h3", //修改为服务端配置的 alpn 标签（选配）。若服务端无此部分配置，那么必须删除此条参数。
  "up_mbps": 10,
  "down_mbps": 50,
  "socks5": {
    "listen": "127.0.0.1:1080"
  },
  "http": {
    "listen": "127.0.0.1:8080"
  }
}
//备注：
//1、此配置仅支持接入服务端的 v1 版。


//Hysteria 的 v2 版配置示例
{
  "server": "xx.yy:443", //修改为服务端分配的域名及端口
  "auth": "diy443", //修改为服务端配置的密码
  "bandwidth": {
    "up": "20 mbps",
    "down": "100 mbps"
  },
  "socks5": {
    "listen": "127.0.0.1:1080"
  },
  "http": {
    "listen": "127.0.0.1:8080"
  }
}
//备注：
//1、此配置仅支持接入服务端的 v2 版。
